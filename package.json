{
  "name": "reviews",
  "version": "0.1.0",
  "description": "Service for PetToyCo that mimics the review module of PetCo.",
  "main": "server.js",
  "scripts": {
    "test": "jest __tests__/componentTests/ --runInBand --silent --coverage",
    "testServer": "jest __tests__/serverEndpointsTests/ --runInBand --silent --coverage",
    "testSeed": "jest server/seed/seed.test.js --coverage",
    "testSDC": "jest __tests__/SDCTests/ --env=node --verbose false --watch",
    "genNoSQL": "node server/seed/generateNoSQL",
    "genSQL": "node server/seed/generateSQL",
    "seedMongo1": "mongoimport --type csv -d PTCReviewsService -c aggregate_reviews --headerline --drop detailedItemReviews.csv",
    "seedMongo2": "mongoimport --type csv -d PTCReviewsService -c individual_reviews --headerline --drop detailedIndividualReviews.csv",
    "server": "node index.js",
    "build": "webpack",
    "start": "node_env=\"development\" nodemon server.js",
    "start:test": "node_env=\"test\" nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PetToyCo/reviews.git"
  },
  "author": "Nick-Zabalza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PetToyCo/reviews/issues"
  },
  "homepage": "https://github.com/PetToyCo/reviews#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.2.1",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "compression-webpack-plugin": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "html-loader": "^1.1.0",
    "jest": "^26.1.0",
    "jest-serial-runner": "^1.1.0",
    "moment": "^2.27.0",
    "nock": "^12.0.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions-assertions": "^1.3.0",
    "redux-mock-store": "^1.5.4",
    "supertest": "^4.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "csv-write-stream": "^2.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "lorem-ipsum": "^2.0.3",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "pg": "^8.3.3",
    "serve-static": "^1.14.1",
    "zlib": "^1.0.5"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "testEnvironment": "jsdom"
  }
}
