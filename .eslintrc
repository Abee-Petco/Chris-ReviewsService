{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "ignorePatterns": ["app.js", "bundle.js", "bundleServerTests.js"],
  "rules": {
    "import/extensions": 0,
    "react/react-in-jsx-scope": 0,
    "no-undef": 0,
    "prefer-arrow-callback": 0,
    "no-unused-expressions": 0,
    "spaced-comment": 0,
    "space-before-function-paren": 0,
    "no-plusplus": 0,
    "max-len": 0,
    "no-else-return": 0,
    "quote-props": 0,
    "func-names": 0,
    "arrow-body-style": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/prop-types": 0,
  }
}